<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>metroui</id>
    <version>5.1.10</version>
    <title>Metro UI Components Library</title>
    <authors>Serhii Pimenov</authors>
    <owners>serhii_pimenov</owners>
    <license type="expression">MIT</license>
    <icon>icon.png</icon>
    <projectUrl>http://metroui.org.ua/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Metro UI is an open source toolkit for developing with HTML, CSS, and JS</description>
    <language>en-US</language>
    <copyright>Copyright 2012-2025, Serhii Pimenov</copyright>
    <tags>Metro UI CSS Framework LESS CSS JS HTML</tags>
    <readme>README.md</readme>
    <releaseNotes>
        
        + [x] Added bulls: `star`, `badge`
        + [x] Updated `textarea`, added attribute `data-initial-height` 
        + [x] Updated `chat`, added attribute `data-send-mode` (values: `button`, `enter`, `control+enter`)
        + [x] Updated `chat`, an input field type changed to `textarea`
        + [x] Updated `chat`, added support "```...```" code blocks
        + [x] Updated `chat`, added support for emoji
        + [x] Fixed `toggle-button` behavior
    </releaseNotes>
    <repository type="git" url="https://github.com/olton/metroui" branch="master"/>
  </metadata>
  <files>
    <file src="CHANGELOG.md" />
    <file src="LICENSE" />
    <file src="README.md" />
    <file src="icon.png" />
    <file src="lib\*.js" target="Content\Scripts" />
    <file src="lib\*.css" target="Content\Content" />
  </files>
</package>