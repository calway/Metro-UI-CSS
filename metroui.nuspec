<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>metroui</id>
    <version>5.1.4</version>
    <title>Metro UI Components Library</title>
    <authors>Serhii Pimenov</authors>
    <owners>serhii_pimenov</owners>
    <license type="expression">MIT</license>
    <icon>icon.png</icon>
    <projectUrl>http://metroui.org.ua/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Metro UI is an open source toolkit for developing with HTML, CSS, and JS</description>
    <language>en-US</language>
    <copyright>Copyright 2012-2025, Serhii Pimenov</copyright>
    <tags>Metro UI CSS Framework LESS CSS JS HTML</tags>
    <releaseNotes>
        5.1.4

        + Updated colors `yellow`, `seashell`, `clown`
        + To `navview` added new CSS vars: `--navview-icon-color`, `--navview-icon-color-hover`, `--navview-icon-color-active`
        + `navview` now uses two chars for icon replacer if caption has two words: for caption `Serhii Pimenov` will be used `SP`
        + Remove default styles for lists: `list-style-type`
        + Fixed the dialog close action for default actions
        + Fixed knob position in `switch` component
    </releaseNotes>
    <repository type="git" url="https://github.com/olton/metroui" branch="master"/>
  </metadata>
  <files>
    <file src="CHANGELOG.md" />
    <file src="LICENSE" />
    <file src="README.md" />
    <file src="icon.png" />
    <file src="lib\*.js" target="Content\Scripts" />
    <file src="lib\*.css" target="Content\Content" />
  </files>
</package>